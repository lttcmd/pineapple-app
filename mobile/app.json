{
  "expo": {
    "name": "Pineapple OFC",
    "slug": "pineapple-ofc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/book.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/book.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B132B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/book.png",
        "backgroundColor": "#0B132B"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/book.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-audio",
      "expo-video"
    ]
  }
}
